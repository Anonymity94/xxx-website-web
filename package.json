{
  "name": "xxx-website-web",
  "version": "1.0.0",
  "private": true,
  "description": "xxx官网",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env umi build",
    "dev": "npm run start:dev",
    "dev:ssr": "cross-env UMI_UI=none NODE_ENV=development concurrently \"umi dev\" \"nodemon server.js\"",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-descriptions": "^1.8.7",
    "@ant-design/pro-form": "^1.31.2",
    "@ant-design/pro-layout": "^6.21.1",
    "@ant-design/pro-list": "^1.10.6",
    "@ant-design/pro-table": "^2.43.4",
    "@umijs/route-utils": "^1.0.37",
    "antd": "^4.16.5",
    "antd-img-crop": "^3.14.2",
    "classnames": "^2.2.6",
    "concurrently": "^6.2.0",
    "jquery": "^3.6.0",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "nodemon": "^2.0.7",
    "omit.js": "^2.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.0.4",
    "reconnectingwebsocket": "^1.0.0",
    "ua-parser-js": "^0.7.28",
    "umi": "^v3.5.0",
    "wangeditor": "^4.7.4",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.4",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/reconnectingwebsocket": "^1.0.7",
    "@types/ua-parser-js": "^0.7.36",
    "@umijs/fabric": "^2.6.2",
    "@umijs/openapi": "^1.1.14",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.19",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
